{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-eval": "off",
        "semi": [1, "always"],
        "no-extend-native": ["error", { "exceptions": ["Array"] }]
    }
}
